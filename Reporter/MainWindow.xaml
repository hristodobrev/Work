        <TextBox Name="txt_password" HorizontalAlignment="Left" Height="23" Margin="77,41,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="77,69,0,0" VerticalAlignment="Top" Width="75" Click="Register_Click"/>
        <TextBlock Name="users_list" HorizontalAlignment="Left" Height="299" Margin="326,10,0,0" Text="test" TextWrapping="Wrap" VerticalAlignment="Top" Width="181"/>
    </Grid>
</Window>
